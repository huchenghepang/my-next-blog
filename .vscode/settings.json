{
  "eslint.options": {
    "overrideConfigFile": "./eslint.config.mjs"
  },
  "commentTranslate.hover.enabled": false,
  "i18n-ally.localesPaths": ["/src/locales"],
  "cSpell.words": ["blogdb"],
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/build": true,
    "**/.next": true,
    "**/.git": true,
    "**/coverage": true,
    "**/.vscode": true,
    "**/logs": true,
    "**/*.log": true,
    "**/tmp": true,
    "**/temp": true,
    "**/public": true,
    "**.yaml": true
  }
}
